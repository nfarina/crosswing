  "scripts": {
    "dev": "vite-node src/manager/server/server.ts",
    "test": "pnpm run prettier && pnpm run circular && pnpm run typecheck && pnpm run vitest",
    "vitest": "vitest --run",
    "test-ui": "vitest --ui --coverage --open=false --api.port 2703",
    "prettier": "prettier --check 'packages/*/src/**/*.ts?(x)'",
    "prettier:fix": "yarn prettier --write",
    "circular": "madge --extensions ts,tsx --circular src",
    "typecheck": "tsc",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev --ci -p 2702",
    "build-storybook": "storybook build",
    "upgrade": "pnpm upgrade -i --latest",
    "node": "vite-node",
    "manager:dev": "vite --config vite.config.ts --port 2750 src/manager/client",
    "manager:build": "cd src/manager/client; vite --config ../../../vite.config.ts build",
    "manager:analyze": "cd src/manager/client; vite-bundle-visualizer",
    "client:dev": "vite --config vite.config.ts --host 0.0.0.0 --port 2701 src/client",
    "client:build": "cd src/client; vite --config ../../vite.config.ts build",
    "client:analyze": "cd src/client; vite-bundle-visualizer"
  },
  "devDependencies": {
