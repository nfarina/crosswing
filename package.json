    "@types/debug": "^4.1.9",
    "@types/minimist": "^1.2.3",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@types/react-is": "^18.2.2",
    "@types/react-transition-group": "^4.4.7",
    "@vitejs/plugin-react": "^4.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "bun-types": "^1.0.3",
    "prettier": "^3.0.3",
    "prettier-check": "^2.0.0",
    "storybook": "^7.4.5",
    "timemachine": "^0.3.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^4.0.0"
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "dayjs": "^1.11.10",
    "debug": "^4.3.4",
    "minimist": "^1.2.8",
    "pidusage": "^3.0.2",
    "ps-tree": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.0.8"
  },
  "files": [
    "src",
    "!src/**/*.test.ts",
    "!src/**/*.stories.ts",
    "!src/**/dist"
  ]
