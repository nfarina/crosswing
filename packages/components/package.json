{
  "name": "@cyber/components",
  "type": "module",
  "sideEffects": false,
  "version": "0.0.0",
  "exports": {
    "./badges/*": "./src/badges/*.js",
    "./desktop/*": "./src/desktop/*.js",
    "./forms/*": "./src/forms/*.js",
    "./labels/*": "./src/labels/*.js",
    "./sites/*": "./src/sites/*.js",
    "./toolbar/*": "./src/toolbar/*.js",
    "./*": "./src/*.js"
  },
  "dependencies": {
    "@cyber/hooks": "workspace:^",
    "@cyber/host": "workspace:^",
    "@cyber/modals": "workspace:^",
    "@cyber/router": "workspace:^",
    "@cyber/shared": "workspace:^",
    "@cyber/theme": "workspace:^",
    "dayjs": "^1.11.9",
    "styled-components": "^6.0.4"
  },
  "files": [
    "src",
    "!src/**/*.test.ts",
    "!src/**/*.stories.ts"
  ]
}