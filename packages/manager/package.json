{
  "name": "@crosswing/manager",
  "type": "module",
  "version": "0.0.3",
  "description": "Crosswing development process manager.",
  "author": "nfarina@gmail.com",
  "bin": {
    "crosswing": "./bin/crosswing"
  },
  "scripts": {
    "dev": "cd client; vite --port 2750",
    "serve": "bun server/server.ts",
    "build": "cd client; vite build",
    "analyze": "cd client; bunx vite-bundle-visualizer",
    "prepack": "cd client; vite build"
  },
  "dependencies": {
    "minimist": "^1.2.8",
    "pidusage": "^3.0.2",
    "ps-tree": "^1.2.0"
  },
  "devDependencies": {
    "@crosswing/hooks": "workspace:*",
    "@crosswing/host": "workspace:*",
    "@crosswing/icons": "workspace:*",
    "@crosswing/modals": "workspace:*",
    "@crosswing/router": "workspace:*",
    "@crosswing/theme": "workspace:*",
    "@vitejs/plugin-react": "^4.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.8",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^4.0.0"
  },
  "files": [
    "**/*.ts?(x)",
    "bin/",
    "!client/",
    "client/dist"
  ]
}